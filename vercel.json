{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
    "headers": [
        {
            "source":"/api/(.*)",
            "headers":[
                {"key": "Acces-Control-Allow-Credentials", "value": "true"},
                {"key": "Acces-Control-Allow-Origin", "value": "*"},
                {"key": "Acces-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"},
                {"key": "Acces-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept"},
            ]
        }
    ]
  }
  
